{% extends 'base.html.twig' %}

{% block body %}
    <h1>Categorieevents list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nomcategorie</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categorieevent in categorieevents %}
            <tr>
                <td><a href="{{ path('categorieevent_show', { 'id': categorieevent.id }) }}">{{ categorieevent.id }}</a></td>
                <td>{{ categorieevent.nomcategorie }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('categorieevent_show', { 'id': categorieevent.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('categorieevent_edit', { 'id': categorieevent.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('categorieevent_new') }}">Create a new categorieevent</a>
        </li>
    </ul>
{% endblock %}
